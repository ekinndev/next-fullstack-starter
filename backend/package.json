{
    "name": "backend-starter-template",
    "version": "1.0.0",
    "description": "This is a dockerized express server template",
    "main": "index.js",
    "scripts": {
        "test": "jest --verbose ./src",
        "test:coverage": "jest --verbose --coverage ./src",
        "start": "NODE_ENV=development node ./bin/www",
        "start:prod": "NODE_ENV=production node ./bin/www",
        "watch": "tsc-watch --outDir ./build  --onSuccess \"node ./bin/www\""
    },
    "author": "Ekin Abalioglu",
    "license": "MIT",
    "dependencies": {
        "async-validator": "^4.0.7",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "debug": "^4.3.2",
        "express": "^5.0.0-alpha.8",
        "mongoose": "^6.1.6",
        "mongoose-autopopulate": "^0.16.0",
        "passport": "^0.6.0",
        "supertest": "^6.2.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.4",
        "@types/node": "^18.0.0",
        "jest": "^28.1.2",
        "prettier": "^2.5.1",
        "ts-jest": "^28.0.5",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.7.4"
    }
}
